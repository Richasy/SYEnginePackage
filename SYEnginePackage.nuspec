<?xml version="1.0"?>
<package >
  <metadata>
    <id>Richasy.SYEnginePackage</id>
    <version>1.0.0</version>
    <title>SYEnginePackage</title>
    <authors>Richasy</authors>
    <owners>Richasy</owners>
    <licenseUrl>https://github.com/amamiya/SYEngine/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/amamiya/SYEngine/</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>The packaged version of SYEngine supports UWP and is suitable for use on Windows 10 and later systems.</description>
    <releaseNotes>First Release.</releaseNotes>
    <tags>SYEngine uwp</tags>
    <dependencies>
      <group targetFramework="uap10.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="SYEngine.winmd" target="lib\uap10.0"/>
    <!-- DLLs and resources -->
    <file src="ARM\*" target="runtimes\win10-arm\native"/>
    <file src="x64\*" target="runtimes\win10-x64\native"/>
    <file src="x86\*" target="runtimes\win10-x86\native"/>
    <file src="ARM64\*" target="runtimes\win10-arm64\native"/>
  </files>
</package>